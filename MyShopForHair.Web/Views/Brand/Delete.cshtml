@{
    ViewData["Title"] = "Brands";
}

@model IEnumerable<BrandViewModel>


<h1 class="text-danger">DELETE</h1>
<div class="list-group">
    @foreach (var brand in Model)
    {
        <a asp-action="Delete" asp-route-id="@brand.Id" class="list-group-item list-group-item-action">@brand.Name</a>
    }
</div>

