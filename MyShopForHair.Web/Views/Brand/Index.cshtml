@{
    ViewData["Title"] = "Brands";
}

@model IEnumerable<BrandViewModel>

<a asp-action="Delete" class="btn btn-danger" >Delete</a>
<a asp-action="Add" class="btn btn-danger">Add</a>


<div class="list-group">
    @foreach (var brand in Model)
    {
        <a asp-action="Index" asp-route-id="@brand.Id" class="list-group-item list-group-item-action">@brand.Name</a>
    }
</div>

